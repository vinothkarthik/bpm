  <process id="ft_process" name="ft_process" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <documentation>Funds Transfer Process Initiated</documentation>
    </startEvent>
    <userTask id="usertask1" name="Inititate FT" activiti:assignee="${csm_initiator}">
