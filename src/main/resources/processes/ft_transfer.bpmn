    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask2" name="Approve Pending" activiti:assignee="${csm_releaser}">
      <documentation>FT Pending Approved and in Ready to Release</documentation>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <userTask id="usertask3" name="Release Wire" activiti:assignee="${wfl_approver}">
      <documentation>FT Release Decision</documentation>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="usertask3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask3" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="usertask2">
      <documentation>FT Rejected, Moved to In Pending</documentation>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!Approved}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="Send JMS to Downstream" activiti:class="com.vin.bpm.jms.service.DownStreamMessaging">
    
    </serviceTask>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Approved}]]></conditionExpression>
    </sequenceFlow>
    <receiveTask id="receivetask1" name="Receive Task">
    <documentation>Receive Task Called</documentation>
    </receiveTask>
    <boundaryEvent id="boundarytimer1" name="Timer" attachedToRef="receivetask1" cancelActivity="true">
      <timerEventDefinition>
        <timeDuration>PT1M</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow8" sourceRef="servicetask1" targetRef="receivetask1">
    <documentation>Receive Task Called</documentation>
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="receivetask1" targetRef="endevent1">
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask2" name="Send Notification to Involved Parties" activiti:class="com.vin.bpm.mail.service.MailSenderDelegate"></serviceTask>
    <sequenceFlow id="flow10" sourceRef="boundarytimer1" targetRef="servicetask2"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow11" sourceRef="servicetask2" targetRef="endevent2"></sequenceFlow>
  </process>
